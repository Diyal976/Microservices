        </line>
        <line>
          <array/>
          <regexp>@^\sroute-target\sexport\s+(?&lt;rt_identifier_exp>[:0-9]+)\s*$@</regexp>
        </line>
        <line>
          <array/>
          <regexp>@^\srd\s+(?&lt;rd>[:0-9]+)\s*$@</regexp>
        </line>
      </lines>
    </parser>
  </command>
  <command name="UPDATE">
    <operation>ip vrf {$params.object_id}no rd {$vrf.$object_id.rd}rd {$params.rd}{if !empty($vrf.$object_id.rt_identifier_imp)}no route-target import {$vrf.$object_id.rt_identifier_imp}{/if}{if !empty($vrf.$object_id.rt_identifier_exp)}no route-target export {$vrf.$object_id.rt_identifier_exp}{/if}{if !empty($params.rt_identifier_imp)}route-target import {$params.rt_identifier_imp}{/if}{if !empty($params.rt_identifier_exp)}route-target export {$params.rt_identifier_exp}{/if}
</operation>
